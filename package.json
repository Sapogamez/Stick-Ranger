{
  "name": "stick-ranger",
  "version": "1.0.0",
  "description": "A modern recreation of the classic Stick Ranger game using TypeScript and React",
  "main": "dist/bundle.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist build",
    "start": "npm run build && npm run serve",
    "serve": "http-server dist -p 3000"
  },
  "keywords": [
    "game",
    "stick-ranger",
    "typescript",
    "react",
    "web-game",
    "rpg"
  ],
  "author": "Stick Ranger Team",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "jest": "^30.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sapogamez/Stick-Ranger.git"
  },
  "bugs": {
    "url": "https://github.com/Sapogamez/Stick-Ranger/issues"
  },
  "homepage": "https://github.com/Sapogamez/Stick-Ranger#readme"
}
